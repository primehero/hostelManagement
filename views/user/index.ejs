<% include ../partials/header %>

<% include ../partials/sidebar %>

<div class="col-md-9">
  <table class="table">
    <thead>
      <tr>
        <th>User Name</th>
        <th>Details</th>
      </tr>
    </thead>
    <tbody>
      <% users.forEach(function(user) { %>
      <tr>
        <td>
          <%= user.username %>
        </td>
        <!-- SHOW -->
        <td>
          <a class="btn btn-primary" href="/user/<%= user._id %>">
            Details
          </a>
        </td>
      </tr>
      <% }) %>
    </tbody>
  </table>
</div><!-- ./col -->


<% include ../partials/footer %>
